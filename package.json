{
    "name": "ngrx-graphql",
    "version": "1.1.2",
    "scripts": {
        "ng": "ng",
        "build": "rm -Rf tmp && rm -Rf dist && ng build && cp LICENSE dist/ngrx-graphql && cp README.md dist/ngrx-graphql",
        "test": "ng test",
        "lint": "ng lint",
        "i:a": "npm run i:a6 && npm run i:a7 && npm run i:a8 && npm run i:a9",
        "i:a6": "cd e2e/angular6 && npm install && npm run postinstall",
        "i:a7": "cd e2e/angular7 && npm install && npm run postinstall",
        "i:a8": "cd e2e/angular8 && npm install && npm run postinstall",
        "i:a9": "cd e2e/angular9 && npm install && npm run postinstall",
        "s:a": "npm run s:a6 && npm run s:a7 && npm run s:a8 && npm run s:a9",
        "s:a6": "P=e2e/angular6/node_modules/ngrx-graphql && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-graphql/. $P",
        "s:a7": "P=e2e/angular7/node_modules/ngrx-graphql && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-graphql/. $P",
        "s:a8": "P=e2e/angular8/node_modules/ngrx-graphql && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-graphql/. $P",
        "s:a9": "P=e2e/angular9/node_modules/ngrx-graphql && rm -Rf $P && mkdir -p $P && cp -R dist/ngrx-graphql/. $P",
        "b:a": "npm run b:a6 && npm run b:a7 && npm run b:a8 && npm run b:a9",
        "b:a6": "cd e2e/angular6 && npm run build -- --prod --progress=false && rm -Rf dist",
        "b:a7": "cd e2e/angular7 && npm run build -- --prod --progress=false && rm -Rf dist",
        "b:a8": "cd e2e/angular8 && npm run build -- --prod --progress=false && rm -Rf dist",
        "b:a9": "cd e2e/angular9 && npm run build -- --prod --progress=false && rm -Rf dist",
        "e2e:a": "npm run e2e:a6 && npm run e2e:a7 && npm run e2e:a8 && npm run e2e:a9",
        "e2e:a6": "cd e2e/angular6 && npm run e2e",
        "e2e:a7": "cd e2e/angular7 && npm run e2e",
        "e2e:a8": "cd e2e/angular8 && npm run e2e",
        "e2e:a9": "cd e2e/angular9 && npm run e2e",
        "e2e": "npm run build && npm run i:a && npm run s:a && npm run e2e:a",
        "test:cid": "ng test ngrx-graphql",
        "test:cid:debug": "ng test ngrx-graphql -- --watch --browsers=Chrome",
        "publish": "cd dist/ngrx-graphql && npm publish",
        "release": "export $(cat .env) && semantic-release"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:satanTime/ngrx-graphql.git"
    },
    "author": "MG. <m@sudo.eu>",
    "license": "Apache-2.0",
    "dependencies": {
        "@angular/common": "6.1.10",
        "@angular/compiler": "6.1.10",
        "@angular/core": "6.1.10",
        "@angular/platform-browser": "6.1.10",
        "@angular/platform-browser-dynamic": "6.1.10",
        "@ngrx/effects": "6.1.2",
        "@ngrx/entity": "6.1.2",
        "@ngrx/store": "6.1.2",
        "core-js": "2.6.12",
        "ngrx-entity-relationship": "1.3.5",
        "rxjs": "6.6.3",
        "zone.js": "0.8.29"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.8.9",
        "@angular-devkit/build-ng-packagr": "0.8.9",
        "@angular/cli": "6.2.9",
        "@angular/compiler-cli": "6.1.10",
        "@angular/language-service": "6.1.10",
        "@commitlint/cli": "11.0.0",
        "@commitlint/config-conventional": "11.0.0",
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/exec": "5.0.0",
        "@semantic-release/git": "9.0.0",
        "@semantic-release/github": "7.2.0",
        "@types/jasmine": "3.6.2",
        "@types/jasminewd2": "2.0.8",
        "@types/node": "12.12.47",
        "codelyzer": "5.2.2",
        "commitlint": "11.0.0",
        "coveralls": "3.1.0",
        "husky": "4.3.6",
        "jasmine-core": "3.6.0",
        "jasmine-spec-reporter": "5.0.2",
        "karma": "5.2.3",
        "karma-chrome-launcher": "3.1.0",
        "karma-coverage-istanbul-reporter": "2.1.1",
        "karma-jasmine": "3.3.1",
        "karma-jasmine-html-reporter": "1.5.4",
        "karma-junit-reporter": "2.0.1",
        "lint-staged": "10.5.3",
        "ng-packagr": "5.7.1",
        "prettier": "2.2.1",
        "protractor": "5.4.4",
        "puppeteer": "3.3.0",
        "rxjs-tslint-rules": "4.34.7",
        "semantic-release": "17.3.0",
        "ts-node": "8.10.2",
        "tsickle": "0.35.0",
        "tslib": "1.13.0",
        "tslint": "5.20.1",
        "tslint-defocus": "2.0.6",
        "tslint-lines-between-class-members": "1.3.6",
        "tslint-microsoft-contrib": "6.2.0",
        "typescript": "2.9.2"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "lint-staged && npm run lint && npm run test",
            "post-commit": "git update-index --again"
        }
    },
    "renovate": {
        "extends": [
            "config:base",
            ":semanticCommitTypeAll(chore)"
        ],
        "rangeStrategy": "pin",
        "rebaseWhen": "conflicted",
        "separateMajorMinor": true,
        "separateMinorPatch": true,
        "separateMultipleMajor": true,
        "ignoreDeprecated": false,
        "semanticCommitScope": "{{#if baseDir}}{{baseDir}}{{else}}deps{{/if}}",
        "additionalBranchPrefix": "{{#if baseDir}}{{baseDir}}/{{else}}root/{{/if}}"
    }
}
